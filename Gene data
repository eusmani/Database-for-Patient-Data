CREATE TABLE genes (
    gene_id INTEGER PRIMARY KEY AUTOINCREMENT,
    symbol TEXT NOT NULL,
    omim_id TEXT,
    total_phenotypes INTEGER,
    omim_entries INTEGER,
    description TEXT
);

CREATE INDEX idx_genes_symbol ON genes(symbol);
CREATE INDEX idx_genes_omim_id ON genes(omim_id);

INSERT INTO genes (symbol, omim_id, total_phenotypes, omim_entries, description)
VALUES ('BRCA1', '113705', 6, 1, 'Breast Cancer, Ovarian Cancer, Prostate Cancer, Pancreatic Cancer, Male Breast Cancer');
