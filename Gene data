CREATE TABLE genes (
	gene_id SERIAL PRIMARY KEY,
	symbol VARCHAR(20) NOT NULL,
	full_name TEXT,
	chromosome VARCHAR(10),
	description TEXT,
	associated_diseases TEXT
);


